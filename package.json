{
  "name": "url_shortener",
  "version": "1.0.0",
  "description": "shorten your url",
  "main": "index.js",
  "type": "module",
  "scripts": {
    "start": "dotenv -e .env.development -- node index.js",
    "migrate:postgres": "dotenv -e .env.test -- npx prisma migrate dev --name postgres-init",
    "migrate:postgres:ci": "dotenv -e .env.ci -- npx prisma migrate dev --name postgres-init",
    "test": "dotenv -e .env.test -- node --experimental-vm-modules node_modules/jest/bin/jest.js",
    "test:ci": "dotenv -e .env.ci -- node --experimental-vm-modules node_modules/jest/bin/jest.js"
  },
  "author": "Wama",
  "license": "ISC",
  "dependencies": {
    "@prisma/client": "^3.11.0",
    "dotenv": "^16.0.0",
    "express": "^4.17.3",
    "mysql": "^2.18.1"
  },
  "devDependencies": {
    "@faker-js/faker": "^6.0.0",
    "dotenv-cli": "^5.1.0",
    "jest": "^27.5.1",
    "nodemon": "^2.0.15",
    "prisma": "^3.11.0",
    "supertest": "^6.2.2"
  },
  "jest": {
    "testEnvironment": "jest-environment-node",
    "transform": {}
  }
}
